model user {
  id             String     @id @default(dbgenerated("gen_random_uuid()"))
  profile_image  String     @default("avatar.jpg")
  cover_image    String     @default("avatar.jpg")
  email          String     @unique
  name           String
  authored_posts posts[]    @relation("author_post")
  likes          like[]     @relation("liker_like")
  profile        profile?
  // friends        friend[]  @relation("friend")
  pages          pages[]
  comments       comments[]
  replies        replies[]
  shares         shares[]
  page_follows   pages[]    @relation("user_follow_pages")
  groups_in      group[]    @relation("user_in_group")

  @@schema("public")
}

model group {
  id          String  @id @default(dbgenerated("gen_random_uuid()"))
  name        String
  description String?
  members     user[]  @relation("user_in_group")
  posts       posts[] @relation("group_post")

  @@schema("public")
}

model profile {
  bio           String
  date_of_birth DateTime @db.Timestamptz(6)
  user_id       String
  user          user     @relation(fields: [user_id], references: [id])

  @@id([user_id])
  @@schema("public")
}

model pages {
  id        String  @id @default(dbgenerated("gen_random_uuid()"))
  user_id   String
  user      user    @relation(fields: [user_id], references: [id])
  followers user[]  @relation("user_follow_pages")
  posts     posts[] @relation("page_post")

  @@schema("public")
}

enum like_type {
  like
  love
  care
  haha
  wow
  sad
  angry

  @@schema("public")
}

enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn

  @@schema("auth")
}
